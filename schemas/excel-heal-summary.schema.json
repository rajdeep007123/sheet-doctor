{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://sheet-doctor.dev/schemas/excel-heal-summary.schema.json",
  "title": "excel-doctor heal summary output",
  "type": "object",
  "required": ["contract", "schema_version", "tool_version", "run_summary", "stats"],
  "properties": {
    "contract": {
      "type": "object",
      "required": ["name", "version"],
      "properties": {
        "name": {"const": "excel_doctor.heal_summary"},
        "version": {"type": "string"}
      }
    },
    "schema_version": {"type": "string"},
    "tool_version": {"type": "string"},
    "run_summary": {"type": "object"},
    "stats": {"type": "object"}
  }
}
